#INCLUDE "Protheus.ch"
#INCLUDE "TOTVS.CH" 

User Function operadoresMatematicos()

    // Operadores Matematicos

    local nSoma := 1 + 1
    local nSubtracao := 1 - 1
    local nDivisao := 1 / 1
    local nExponenciacao := 2**2
    local nModulo := 3 % 2

    ConOut(nSoma)
    ConOut(nSubtracao)
    ConOut(nDivisao)
    ConOut(nExponenciacao)
    ConOut(nModulo)

return
    
